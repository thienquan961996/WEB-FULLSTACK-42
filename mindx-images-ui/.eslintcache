nder, setFirstRender] = useState(true)\n\n  const verifyAuth = async () =>{\n    setLoading(true);\n    try {\n      const res = await api({\n        url: '/auth/verify',\n        method: 'GET'\n    });\n      \n      if(res.success) {\n      setUser(res.data)\n      setLoading(false);\n    }else{\n      setLoading(false);\n    }\n  }catch (err) {\n    setLoading(false);\n    }\n  }\n\n  useEffect(() =>{\n    const token = localStorage.getItem('token');\n    if(token) {\n      verifyAuth();\n    }\n    setFirstRender(false);\n    //  else{\n    //   setLoading(false);\n    // }\n  }, [])\n\n  const login = ({ user, token }) =>{\n    localStorage.setItem('token', token);\n    setUser(user);\n  }\n\n  const logout = () =>{\n    localStorage.removeItem('token');\n    setUser(null);\n  }\n\n  if (loading || firstRender.current ) return <div>Loading...</div>\n\n  const authValue = {\n    user,\n    login,\n    logout\n  };\n\n  return (\n    <AuthContext.Provider value={authValue}>\n      <div className=\"App\">\n        <Switch>\n          <Route path='/' exact>\n            <Home />\n          </Route>\n          <Route path='/login' exact>\n            <Login />\n          </Route>\n          <Route path='/signup' exact>\n            <Signup />\n          </Route>\n          <Route path='/upload' exact>\n            <CreatePost />\n          </Route>\n          <Route path='/posts/:id' exact>\n            <PostDetail />\n          </Route>\n          <Route path='*' exact>\n            <NotFound />\n          </Route>\n        </Switch>\n      </div>\n    </AuthContext.Provider>\n  );\n}\n\nexport default App;\n","C:\\mindx\\New folder\\WEB-FULLSTACK-42\\mindx-images-ui\\src\\reportWebVitals.js",[],"C:\\mindx\\New folder\\WEB-FULLSTACK-42\\mindx-images-ui\\src\\page\\Home\\Home.js",["76"],"import { useEffect, useState } from 'react';\r\nimport { useHistory } from 'react-router-dom';\r\nimport { Row, Col, Container } from 'react-bootstrap';\r\nimport MainLayout from '.